{
  "extends": [
    "airbnb",
    "prettier",
    "plugin:node/recommended",
    "eslint:recommended"
  ],
  "plugins": ["prettier"],
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "spaced-comment": "off",
    "no-console": "warn",
    "consistent-return": "off",
    "func-names": "off",
    "object-shorthand": "off",
    "no-process-exit": "off",
    "no-param-reassign": "off",
    "no-return-await": "off",
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    " parser": "babel-eslint",
    // "parserOptions": {
    //   "sourceType": "module",
    //   "allowImportExportEverywhere": "true"
    // },
    "prefer-destructuring": ["error", { "object": true, "array": false }],
    "no-unused-vars": [1, { "argsIgnorePattern": "req|res|next|val" }]
  }
}
